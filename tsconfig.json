{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",                                  
    "module": "commonjs",                                
    "esModuleInterop": true, 
    "lib": [
        "es5", "es6", "dom", "dom.iterable"
    ],                           
    "forceConsistentCasingInFileNames": true,        
    "strict": true,                                      
    "skipLibCheck": true   ,                           
        "outDir": "./dist",
        "allowJs": true,
    },
        "include": [
        "src"
    ]
    
}

